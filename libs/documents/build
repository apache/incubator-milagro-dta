#build the protobuffer docs.pb.go file from the definition file docs.proto
protoc \
  --proto_path=$GOPATH/src \
  --proto_path=$GOPATH/src/github.com/gogo/protobuf/ \
  --proto_path=. \
  --go_out=. \
  --govalidators_out=. \
  *.proto

